{
  "name": "middleman-boilerplate",
  "version": "1.0.0",
  "description": "A simple middleman project integrated with HEx",
  "repository": "git@github.com:bitnami/middleman-boilerplate.git",
  "author": "Bitnami <webdev@bitnami.com>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "node-sass-chokidar --include-path ./node_modules ./source/assets/stylesheets/main.scss -o ./build/assets/stylesheets --watch",
    "build": "node-sass-chokidar --include-path ./node_modules ./source/assets/stylesheets/main.scss -o ./build/assets/stylesheets --output-style compressed"
  },
  "dependencies": {
    "@bitnami/hex": "^3.2.0",
    "node-sass-chokidar": "^1.3.3"
  }
}
